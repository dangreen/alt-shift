{
  "name": "altshift",
  "type": "module",
  "author": "Dan Onoshko <danon0404@gmail.com>",
  "license": "ISC",
  "scripts": {
    "clear": "del ./dist ./storybook-static",
    "dev": "vite",
    "storybook": "storybook dev -p 6006 --no-open",
    "build:sprite": "node ./scripts/svg-sprite.js",
    "build": "run tsc [ vite build ]",
    "build:storybook": "storybook build",
    "preview": "vite preview",
    "lint:styles": "stylelint 'app/**/*.css'",
    "lint:scripts": "eslint --flag v10_config_lookup_from_file",
    "lint": "run -p lint:styles lint:scripts",
    "format:styles": "stylelint 'app/**/*.css' --fix",
    "format:scripts": "eslint --fix --flag v10_config_lookup_from_file",
    "format": "run -p format:styles format:scripts",
    "test:types": "tsc",
    "test": "run -p lint test:types",
    "commit": "cz",
    "updateGitHooks": "simple-git-hooks"
  },
  "dependencies": {
    "@kidajs/react": "1.0.0-alpha.3",
    "@kidajs/react-router": "1.0.0-alpha.1",
    "@kidajs/router": "1.0.0-alpha.4",
    "clsx": "^2.1.1",
    "kida": "1.0.0-alpha.5",
    "modern-normalize": "^3.0.1",
    "nanoid": "^5.1.6",
    "openai": "^6.9.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/cz-commitlint": "^20.1.0",
    "@storybook/react": "^10.0.7",
    "@storybook/react-vite": "^10.0.7",
    "@trigen/eslint-config": "^8.2.0",
    "@trigen/scripts": "^8.0.4",
    "@trigen/stylelint-config": "^8.0.0",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "@types/svg-sprite": "^0.0.39",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "commitizen": "^4.3.1",
    "del-cli": "^7.0.0",
    "eslint": "9.31.0",
    "nano-staged": "^0.8.0",
    "postcss-custom-media": "^11.0.6",
    "simple-git-hooks": "^2.13.1",
    "storybook": "^10.0.7",
    "stylelint": "^16.25.0",
    "svg-sprite": "^2.0.4",
    "typescript": "^5.9.3",
    "vite": "^7.2.2"
  }
}
